create table ordenes(
	id_orden int not null auto_increment primary key,
    fechadeRecibido date,
    fechaLimiteEntrega date, 
    fechaEntrega date,
    estado varchar(15),
    observacion text,
    id_cliente int,
    tipoEntrega varchar(30) unique,
    urgente enum("SI","NO"),
	foreign key (id_cliente) references Clientes (id_cliente)
);

