create table Pagos(
	id_pagos int not null primary key,
    numeroTarjeta varchar(20),
    CVC int,
    fechaExpiracion VARCHAR(10),
	nombre varchar(20),
    apellido varchar(20),
    nombre_usuario VARCHAR(25),
    FOREIGN KEY (nombre_usuario) REFERENCES Usuario(nombre_usuario)
);

