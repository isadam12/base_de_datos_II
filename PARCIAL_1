    CREATE TABLE Pagos (
    id_cliente INT, 
    numeroFactura VARCHAR(50) PRIMARY KEY,
    fechaPago DATE,
    totalPago DOUBLE,
    FOREIGN KEY (id_cliente) REFERENCES Clientes(id_cliente),
    metodoPago ENUM('transferencia', 'tarjetaCredito', 'Yappy', 'efectivo'),
    codigogenPago varchar(10) unique
    );
    
